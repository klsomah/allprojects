@page
@model EducationalFundingCo.Pages.Contract.IndexModel

@{
    ViewData["Title"] = "Index";
    Layout = "~/Pages/Shared/_LayoutMain.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Contract[0].FirstName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contract[0].LastName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contract[0].MiddleName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contract[0].Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contract[0].IpAddress)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contract[0].SocialSecurityNum)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contract[0].AcceptedDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contract[0].CompletedDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contract[0].EstCompletedDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contract[0].PaymentFenquency)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contract[0].MaxDelinquencyAge)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contract[0].AcademyProgram)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Contract[0].IdentityUser)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.Contract) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MiddleName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IpAddress)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SocialSecurityNum)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AcceptedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CompletedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstCompletedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PaymentFenquency)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaxDelinquencyAge)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AcademyProgram.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdentityUser.Id)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.Id">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
